
<div class="container edit">
<h2>編集</h2>

  <%= form_for @user_type, :url => {:action => "update" }, html: { method: :put } do |f| %>

  <div class="field user_type short">
    <%= f.label :タイプ %><br />
    <%= f.number_field :user_type, required: true, autofocus: true, :class => "form-control my-form", autocomplete: "user_type" %>
  </div>

  
  <div class="field type_name short">
    <%= f.label :タイプ名 %><br />
    <%= f.text_field :type_name, required: true, :class => "form-control my-form", autocomplete: "type_name" %>
  </div>

  <div class="field price short">
    <%= f.label :金額 %><br />
    <%= f.text_field :price, required: true, :class => "form-control my-form", autocomplete: "price" %>
  </div>

  <div class="actions">
    <%= f.submit "Update", class:"btn btn-primary" %>
  </div>
<% end %>

<%= link_to "Back", :back %>
</dir>