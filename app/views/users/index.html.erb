<div class="container">
    <h2 class="my-reserved-title">お客様一覧</h2>
    
        <table class="table table-bordered table-schedule">
          <thead>
            <tr>
              <th scope="col">名前</th>
              <th scope="col">メール</th>
              <th scope="col">電話</th>
              <th scope="col">生年月日</th>
              <th scope="col">性別</th>
              <th scope="col">紹介者</th>
              <th scope="col">タイプ</th>
            </tr>
          </thead>
          <tbody>
            <% @users.each do |user| %>
               <tr>
                  <td><%= user.name %></td>
                  <td><%= user.email %></td>
                  <td><%= user.tel %></td>
                  <td><%= user.birthday %></td>
                  <td><%= Global::GENDER[user.gender] %></td>
                  <td><%= user.introducer %></td>
                  <td><%= user.user_type %></td>
               </tr>
            <% end %>
          </tbody>
        </table>
</div>